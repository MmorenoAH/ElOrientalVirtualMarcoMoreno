@model List<ModuloVirtual>
<h1>Crear Usuario</h1>

<p>
    <a asp-action="Crear" class="btn btn-success">Crear</a>
    <a asp-controller="Propietario" asp-action="Crear" class="btn btn-success">Agregar Propietario</a>
</p>

<table class="table table-striped table-responsive">
    <thead class="table-dark">
        <tr>
            <th>Id</th>
            <th>Propietario</th>
            <th>Descripcion</th>
            <th>Fecha de Creacion</th>
        </tr>
    </thead>
    <tbody>
       @foreach (var mod in Model)
        {
            <tr>
                <td>@Html.DisplayFor(m => mod.IdModulo)</td>
                <td>@Html.DisplayFor(m => mod.Propietario.NombrePropietario)</td>
                <td>@Html.DisplayFor(m => mod.DescripcionModulo)</td>
                <td>@Html.DisplayFor(m => mod.FechaCreacion)</td>
            </tr>
        }
    </tbody>
</table>